{
  "expo": {
    "name": "image-app",
    "slug": "image-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.angelena.image-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "title": "Image Recognition App",
    "description": "Identify objects in images using your camera or photo library - built for iOS, Android and web",
    "primaryColor": "teal",
    "githubUrl": "https://github.com/chelsea-angelena-freshworks/Image_app2.0"
  }
}
